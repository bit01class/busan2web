<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>day46</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>jsonList</servlet-name>
  	<servlet-class>com.bit.emp.JsonController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>jsonInsert</servlet-name>
  	<servlet-class>com.bit.emp.AddController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>jsonOne</servlet-name>
  	<servlet-class>com.bit.emp.OneController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>jsonOne</servlet-name>
  	<url-pattern>/emp/one.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>jsonList</servlet-name>
  	<url-pattern>/emp/emp.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>jsonInsert</servlet-name>
  	<url-pattern>/emp/add.do</url-pattern>
  </servlet-mapping>
</web-app>